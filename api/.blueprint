models:
    User: { name: string, email: string, email_verified_at: 'timestamp nullable default:NULL', password: string, remember_token: 'string:100 nullable default:NULL' }
    ParlSession: { uuid: uuid, externalId: string, number: integer, name: string, abbreviation: string, startDate: datetime, endDate: datetime, title: 'string nullable', type: 'integer nullable', typeName: 'string nullable', modified: datetime, legislativePeriodNumber: integer, relationships: { hasMany: 'Transcript, Member' } }
    Council: { uuid: uuid, externalId: integer, name: string, abbreviation: string, relationships: { hasMany: 'Member, Transcript' } }
    Party: { uuid: uuid, externalId: integer, name: string, abbreviation: string, relationships: { hasMany: Member } }
    ParlGroup: { uuid: uuid, externalId: integer, number: integer, isActive: boolean, code: string, name: string, abbreviation: string, nameUsedSince: 'datetime nullable', modified: datetime, colour: 'string:8 nullable', relationships: { hasMany: Member } }
    Canton: { externalId: integer, number: integer, name: string, abbreviation: string, relationships: { hasMany: Member } }
    Member: { uuid: uuid, externalId: integer, externalPersonId: integer, isActive: boolean, firstName: string, lastName: string, genderAsString: string, dateJoining: datetime, dateLeaving: datetime, dateElection: datetime, relationships: { belongsTo: 'Party, ParlGroup, Canton, Council', belongsToMany: ParlSession } }
    Business: { uuid: uuid, externalId: integer, shortNumber: string, type: integer, typeName: string, typeAbbreviation: string, title: string, description: string, relationships: { hasMany: Transcript, belongsToMany: Tag } }
    Tag: { uuid: uuid, ExternalId: integer, name: string, relationships: { hasMany: Business } }
    Transcript: { uuid: uuid, externalId: integer, text: string, start: datetime, end: datetime, languageOfText: string, business_id: 'integer nullable', relationships: { belongsTo: 'ParlSession, Council, Member, Business' } }
